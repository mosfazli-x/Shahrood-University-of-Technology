<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="400.0" prefWidth="256.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
    <children>
      <Pane prefHeight="374.0" prefWidth="214.0">
         <children>
              <Button fx:id="ScoresTableButton" defaultButton="true" layoutX="39.0" layoutY="178.0" mnemonicParsing="false" onAction="#ScoresTableButtonClicked" text="Scores Table" textAlignment="CENTER" textFill="#181be1">
                  <font>
                      <Font name="Arial Bold" size="18.0" />
                  </font>
              </Button>
            <Line endX="100.0" layoutX="107.0" layoutY="-1.0" startX="-100.0" />
            <Line endX="-114.5" endY="340.5" layoutX="120.0" layoutY="33.0" startX="-113.5" startY="-32.5" />
            <Line endX="92.5" endY="-23.5" layoutX="115.0" layoutY="23.0" startX="91.5" startY="350.5" />
            <Line endX="100.0" layoutX="106.0" layoutY="374.0" startX="-100.0" />
              <Text fill="#8c227e" layoutX="34.0" layoutY="112.0" smooth="false" stroke="#00e11e" strokeLineCap="BUTT" strokeWidth="0.0" text="Tetris Game" textAlignment="CENTER">
                  <font>
                      <Font name="Tahoma Bold" size="24.0" />
                  </font>
               <effect>
                  <Glow />
               </effect>
              </Text>
              <Button fx:id="NewGameButton" defaultButton="true" layoutX="49.0" layoutY="131.0" mnemonicParsing="false" onAction="#NewGameButtonClicked" text="New Game" textAlignment="CENTER" textFill="#026810" textOverrun="WORD_ELLIPSIS">
                  <font>
                      <Font name="Arial Bold" size="18.0" />
                  </font>
              </Button>
              <Button fx:id="ExitButton" defaultButton="true" layoutX="78.0" layoutY="222.0" mnemonicParsing="false" onAction="#ExitButtonClicked" text="Exit" textAlignment="CENTER" textFill="#da0101">
                  <font>
                      <Font name="Arial Bold" size="18.0" />
                  </font>
              </Button>
         </children>
      </Pane>
    </children>
</FlowPane>
